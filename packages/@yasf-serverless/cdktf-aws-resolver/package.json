{
  "name": "@yasf-serverless/cdktf-aws-resolver",
  "version": "0.1.0",
  "main": "./lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YaSF-serverless/yasf.git"
  },
  "author": "Mikael Giacomini",
  "license": "Apache License 2.0",
  "scripts": {
    "prepare": "yarn build",
    "build": "tsc",
    "generate-barrels": "barrelsby --delete -d src"
  },
  "files": [
    "lib/**/*"
  ],
  "exports": {
    ".": "./lib/index.js",
    "./": "./lib/"
  },
  "dependencies": {
    "@cdktf/provider-aws": "^9.0.52",
    "@cdktf/provider-generator": "^0.12.3",
    "@yasf-serverless/core": "^0.1.0",
    "@npmcli/ci-detect": "^1.4.0",
    "cdktf": "^0.12.2",
    "cdktf-cli": "0.13.1",
    "follow-redirects": "^1.15.2",
    "node-abort-controller": "^3.0.1",
    "pkg-up": "^3.1.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.0.2",
    "@commitlint/config-conventional": "^16.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.2",
    "@semantic-release/npm": "^9.0.0",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^17.0.8",
    "barrelsby": "^2.4.0",
    "husky": "^7.0.0",
    "semantic-release": "^19.0.3",
    "typescript": "^4.5.4"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ],
      "@semantic-release/github"
    ]
  }
}
